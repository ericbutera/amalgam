type Feed {
  id: ID!
  url: String!
  name: String!
}

type Article {
  id: ID!
  title: String!
  content: String!
}

type Query {
  feeds: [Feed!]!
  feed(id: ID!): Feed
  articles(feedId: ID!): [Article!]!
  article(id: ID!): Article
}

type Mutation {
  addFeed(url: String!, name: String!): Feed!
  updateFeed(id: ID!, url: String, name: String): Feed!
  deleteFeed(id: ID!): Feed!
}
